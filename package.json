{
  "name": "@tknf-labs/html-helper",
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "version": "0.1.0",
  "license": "MIT",
  "description": "HTML helper for Backend and Frontend",
  "author": "TKNF LLC <dev@tknf.net>",
  "homepage": "https://github.com/tknf/html-helper",
  "repository": {
    "type": "git",
    "url": "https://github.com/tknf/html-helper"
  },
  "bugs": {
    "url": "https://github.com/tknf/html-helper/issues"
  },
  "contributors": [
    {
      "name": "Tasuku Kuribayashi",
      "email": "Kuribayashi@tknf.net"
    }
  ],
  "keywords": [
    "hono",
    "html",
    "helper",
    "template",
    "javascript",
    "typescript"
  ],
  "publishConfig": {
    "access": "public",
    "@tknf-labs:registry": "https://registry.npmjs.org"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch:coverage": "vitest --coverage",
    "lint": "eslint *.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@tknf-labs/eslint-config": "^3.0.0",
    "@tknf-labs/prettier-config": "^2.2.0",
    "@vitest/coverage-istanbul": "2.1.3",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.3"
  },
  "prettier": "@tknf-labs/prettier-config"
}
